{
  "name": "minepedia-server",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsnd --no-notify --respawn --transpile-only ./src",
    "build": "tsc",
    "test": "dotenv -e .env.test -- jest",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "debug": "tsnd --inspect --respawn --transpile-only ./src",
    "prebuild": "run-s clean generate:*",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "seed": "dotenv -- ts-node prisma/seed.ts",
    "clean": "rimraf dist",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only ./src/schema",
    "db:save": "yarn prisma migrate save --experimental",
    "db:up": "yarn prisma migrate up --experimental",
    "prisma": "dotenv -- prisma",
    "studio": "yarn prisma studio"
  },
  "dependencies": {
    "@nexus/schema": "^0.17.0",
    "@prisma/client": "^2.10.0",
    "@types/image-to-base64": "^2.1.0",
    "@types/jwt-decode": "^2.2.1",
    "apollo-server": "^2.13.0",
    "axios": "^0.21.1",
    "bcrypt": "5.0.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.1.0",
    "file-type": "^16.0.0",
    "graphql": "^15.3.0",
    "graphql-middleware": "^4.0.2",
    "graphql-shield": "7.3.6",
    "image-to-base64": "^2.1.1",
    "js-base64": "^3.6.0",
    "jsonwebtoken": "8.5.1",
    "jwt-decode": "^3.0.0",
    "minio": "^7.0.16",
    "nexus-plugin-prisma": "^0.23.0",
    "typescript-base64-arraybuffer": "^0.1.0"
  },
  "devDependencies": {
    "@prisma/cli": "^2.9.0",
    "@types/bcrypt": "3.0.0",
    "@types/eslint": "^7.2.0",
    "@types/jest": "^26.0.13",
    "@types/jsonwebtoken": "8.5.0",
    "@types/minio": "^7.0.6",
    "@types/node": "^14.10.2",
    "@types/prettier": "^2.0.0",
    "@types/rimraf": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "cross-env": "^7.0.2",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.10.1",
    "graphql-request": "1.8.2",
    "husky": ">=4.3.0",
    "jest": "^26.2.2",
    "jest-environment-node": "^26.2.0",
    "lint-staged": ">=10.4.0",
    "nanoid": "^3.1.12",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.0",
    "ts-jest": "^26.1.4",
    "ts-node": "^9.0.0",
    "ts-node-dev": "1.0.0-pre.63",
    "typescript": "4.0.3",
    "typesync": "^0.7.0"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": "npm run lint",
    "*.{ts,js,json,md}": "prettier --write"
  },
  "resolutions": {
    "fs-capacitor": "^6.2.0",
    "graphql-upload": "^11.0.0"
  }
}
